def max_visited_speciality(patient_medical_speciality_list,medical_speciality):
    index=1
    check_index=1
    count=0
    max1=0
    max1_spec=""
    while index<len(patient_medical_speciality_list):
        count=0
        while check_index<len(patient_medical_speciality_list):
            if patient_medical_speciality_list[check_index]==patient_medical_speciality_list[index]:
                count= count+1
            check_index+=1
        if count>max1:
            max1_spec=patient_medical_speciality_list[index]
        index+=1
    return medical_speciality[max1_spec]

#provide different values in the list and test your program
patient_medical_speciality_list=[301,'P',302, 'P' ,305, 'P' ,401, 'E' ,656, 'E']
medical_speciality={"P":"Pediatrics","O":"Orthopedics","E":"ENT"}
speciality=max_visited_speciality(patient_medical_speciality_list,medical_speciality)
print(speciality)
