#PF-Assgn-34
def find_pairs_of_numbers(num_list,n):
    count=0
    for num in range(0,len(num_list)):
        for num1 in range(num+1,len(num_list)):
            if (num_list[num]+num_list[num1]==n):
                count=count+1
    return count

num_list=[1, 2, 4, 5, 6]
n=6
print(find_pairs_of_numbers(num_list,n))
