#PF-Assgn-44

def find_duplicates(list_of_numbers):
    #start writing your code here
    list_of_duplicates=[]
    for index in range(0,len(list_of_numbers)):
        for index1 in range(index+1,len(list_of_numbers)):
            if list_of_numbers[index]==list_of_numbers[index1] and list_of_numbers[index] not in list_of_duplicates:
                list_of_duplicates.append(list_of_numbers[index])
    return list_of_duplicates

list_of_numbers=[1,2,2,3,3,3,4,4,4,4]
list_of_duplicates=find_duplicates(list_of_numbers)
print(list_of_duplicates)
