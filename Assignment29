def calculate(distance,no_of_passengers):
    profit_earned=0
    total_fuel_cost=(distance*7)
    total_ticket_fare=no_of_passengers*80
    
    if (total_ticket_fare>=total_fuel_cost):
        profit_earned=total_ticket_fare-total_fuel_cost
    else:
        profit_earned=-1
    return profit_earned



#Provide different values for distance, no_of_passenger and test your program
distance=25
no_of_passengers=25
print(calculate(distance,no_of_passengers))
