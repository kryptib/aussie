#PF-Assgn-46

def nearest_palindrome(number):
    count=0
    check=number+1
    while count!=1:
        reverse_number=""
        original_number=list(str(check))
        for index in range(1,len(original_number)+1):
            reverse_number=reverse_number+original_number[index*-1]
        if int(reverse_number)==check:
            count=1
        else:
            check+=1
    return check

number=12300
print(nearest_palindrome(number))
