#PF-Assgn-43

def find_smallest_number(num):
    #start writing your code here
    total=0
    check=0
    while total!=num:
        check+=1
        total=0
        for index in range(1,check+1):
            if(check%index==0):
                total+=1
        if total==num:
            break
    return check

num=1
print("The number of divisors :",num)
result=find_smallest_number(num)
print("The smallest number having",num," divisors:",result)
